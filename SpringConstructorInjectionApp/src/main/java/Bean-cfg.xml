<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd">

<!-- Book b1 = new Book() -->



      <bean id="b1" class="app.model.Book">
        <constructor-arg name="bookId" index="0" type="Integer" value="200"></constructor-arg>
        <constructor-arg name="bookName" index="1" type="String" value="DBMS"></constructor-arg>
        <constructor-arg name="version" index="2" type="Float" value="1.0"></constructor-arg>
       </bean>
       
      
       <bean id="b2" class="app.model.Book">
         <constructor-arg name="bookName" index="0" type="String" value="Networking"></constructor-arg>
        <constructor-arg name="bookId" index="1" type="Integer" value="300"></constructor-arg>
        <constructor-arg name="version" index="2" type="Float" value="1.0"></constructor-arg>
           
       </bean>
      
      
</beans>
