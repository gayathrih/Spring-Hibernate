<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd">

<!-- Book b1 = new Book() -->



      <bean id="b1" class="app.model.Book">
         <property name="bookId" value="100"/> 
          <property name="bookName" value="J2EE"/> 
           <property name="version" value="14.0"/> 
           
       </bean>
       
      
       <bean id="b2" class="app.model.Book">
         <property name="bookId" value="101"/> 
          <property name="bookName" value="Python"/> 
           <property name="version" value="1.0"/> 
           
       </bean>
      
      
</beans>
